<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_METHODS" value="true" />
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="f26522" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bquestion\b.*">
      <option name="FOREGROUND" value="61b442" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>